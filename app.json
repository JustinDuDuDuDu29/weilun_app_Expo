{
  "expo": {
    "name": "崴倫車隊",
    "slug": "weilun_app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/LOGO.jpg",
    "splash": {
      "image": "./assets/LOGO.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.justindudududu29.weilunapp",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LOGO.jpg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.justindudududu29.weilunapp"
    },
    "web": {
      "favicon": "./assets/LOGO.jpg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "...",
          "cameraPermission": "！"
        }
      ]
    ],
    "extra": {
      "owner": "justindu",
      "eas": {
        "projectId": "77a11647-892e-4eeb-a72d-da9c246d9a86"
      }
    }
  }
}
